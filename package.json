    "mongodb": "^3.5.9",
    "serve": "^11.3.2",
    "sinon": "^9.2.4",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typedoc": "^0.19.1",
    "typescript": "^4.8.4",
    "uuid": "^8.2.0"
