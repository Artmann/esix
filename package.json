    "format": "turbo format",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.5",
    "typedoc": "^0.28.1",
    "typedoc-github-theme": "^0.2.1",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
