{
  "name": "esix-workspace",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build-api-docs": "typedoc --options ./packages/esix/typedoc.json --tsconfig ./packages/esix/tsconfig.json --out ./packages/website/public/api ./packages/esix/src/",
    "dev": "turbo dev",
